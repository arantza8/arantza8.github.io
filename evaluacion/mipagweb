  <head>                                  <!-- abro la cabecera  -->
    <!-- contenidos de la cabecera del documento                             -->
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <title>Arantza San Julian</title>
  </head>                                 <!-- cierr la cabecera-->
  <body>                                  <!-- abro el cuerpo del documento     -->
  <!-- contenidos del cuerpo del documento                                   -->
    <p>bla bla bla<br>
    </p>
  </body>                                <!-- cierro el cuerpo del documento    -->
</html>                                  <!-- cierro el contenido html          -->
